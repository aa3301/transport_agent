# Database (MySQL async)
MYSQL_ASYNC_URL=mysql+aiomysql://root:rootpass@mysql:3306/transport_db

# Redis
REDIS_URL=redis://redis:6379/0

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/

# Kafka brokers (comma-separated)
KAFKA_BROKERS=kafka:9092

# JWT secret (change in production)
JWT_SECRET=change_me_to_secure_value

# Groq API key (optional, for LLM)
GROQ_API_KEY=

# Weather API key (add your provider key here if needed)
WEATHER_API_KEY=

# Other
FAISS_K=8
EMBEDDING_MODEL=all-MiniLM-L6-v2
